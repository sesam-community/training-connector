[
  {
    "_id": "{{@ system @}}-orderline-all-lookup",
    "source": {
      "dataset": "{{@ system @}}-orderline-all",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "copy",
              "properties"
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "operation": "lines",
        "replace_entity": false,
        "system": "{{@ system @}}",
        "trace": true,
        "type": "rest"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-orderline-all",
    "source": {
      "dataset": "{{@ system @}}-order-all",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "create_rows": [
            [
              "add",
              "::_id",
              [
                "string",
                "_S.id"
              ]
            ],
            [
              "add",
              "::properties",
              [
                "dict",
                "datatype",
                [
                  "concat",
                  "order/orderline/",
                  [
                    "string",
                    "_S.id"
                  ]
                ]
              ]
            ]
          ],
          "default": [
            [
              "create-child",
              [
                "apply",
                "create_rows",
                "_S.orderLines"
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "type": "emit_children"
      }
    ],
    "type": "pipe"
  }
]