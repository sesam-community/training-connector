[
  {
    "_id": "{{@ system @}}-orderline-collect",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-orderline-all-2",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "filter",
              [
                "neq",
                "_S._deleted",
                true
              ]
            ],
            [
              "copy",
              "*"
            ],
            [
              "add",
              "properties",
              [
                "dict",
                "datatype",
                "order/orderline"
              ]
            ],
            [
              "add",
              "entity",
              [
                "dict",
                "id",
                "_S.id"
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "operation": "lookup",
        "replace_entity": true,
        "side_effects": false,
        "system": "{{@ system @}}",
        "type": "rest"
      },
      {
        "rules": {
          "default": [
            [
              "comment",
              "TODO should use id-expression and payload-property in rest transform"
            ],
            [
              "add",
              "_id",
              [
                "string",
                "_S.value.id"
              ]
            ],
            [
              "merge",
              "_S.value"
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-orderline-all",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-order-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "create_rows": [
            [
              "add",
              "_id",
              [
                "string",
                "_S.id"
              ]
            ],
            [
              "add",
              "id",
              "_S.id"
            ],
            [
              "add",
              "$original_id",
              "_R._S._id"
            ],
            [
              "add",
              "version",
              "_P._S.version"
            ]
          ],
          "default": [
            [
              "create-child",
              [
                "apply",
                "create_rows",
                "_S.orderLines"
              ]
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-orderline-all-2",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-orderline-all",
      "type": "dataset"
    },
    "transform": [
      {
        "type": "emit_children"
      }
    ],
    "type": "pipe"
  }
]