[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-share",
    "batch_size": 1,
    "namespaced_identifiers": false,
    "sink": {
      "deletion_tracking": false,
      "set_initial_offset": "onload"
    },
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-transform",
      "type": "dataset"
    },
    "transform": {
      "properties": {
        "operation_delete": "{{@ datatype @}}-delete",
        "operation_delete_properties": {
          "datatype": "{{@ datatype @}}"
        },
        "operation_insert": "{{@ datatype @}}-insert",
        "operation_insert_properties": {
          "datatype": "{{@ datatype @}}"
        },
        "operation_lookup": "{{@ datatype @}}-lookup",
        "operation_lookup_properties": {
          "datatype": "{{@ datatype @}}"
        },
        "operation_update": "{{@ datatype @}}-update",
        "operation_update_properties": {
          "datatype": "{{@ datatype @}}"
        },
        "primary_key": "id",
        "rest_system": "{{@ system @}}",
        "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
      },
      "template": "transform-share-rest",
      "type": "template"
    },
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "exclude_completeness": [
      "{{@ system @}}-{{@ datatype @}}-share"
    ],
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all2",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "filter",
              [
                "neq",
                "_S._deleted",
                true
              ]
            ],
            [
              "copy",
              "*"
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "operation": "{{@ datatype @}}-lookup",
        "properties": {
          "datatype": "order/{{@ datatype @}}"
        },
        "replace_entity": true,
        "system": "{{@ system @}}",
        "type": "rest"
      },
      {
        "rules": {
          "default": [
            [
              "comment",
              "TODO should use id-expression and payload-property in rest transform"
            ],
            [
              "add",
              "_id",
              [
                "string",
                "_S.value.id"
              ]
            ],
            [
              "merge",
              "_S.value"
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "properties": {
          "primary_key": "id",
          "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
        },
        "template": "transform-collect-rest",
        "type": "template"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-order-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "create_rows": [
            [
              "add",
              "_id",
              [
                "string",
                "_S.id"
              ]
            ],
            [
              "add",
              "id",
              "_S.id"
            ],
            [
              "add",
              "$original_id",
              "_R._S._id"
            ],
            [
              "add",
              "version",
              "_P._S.version"
            ]
          ],
          "default": [
            [
              "create-child",
              [
                "apply",
                "create_rows",
                "_S.orderLines"
              ]
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all2",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all",
      "type": "dataset"
    },
    "transform": [
      {
        "type": "emit_children"
      }
    ],
    "type": "pipe"
  }
]
