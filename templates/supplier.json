{
  "_id": "{{@ system @}}-{{@ datatype @}}-collect",
  "exclude_completeness": [
    "{{@ system @}}-{{@ datatype @}}-share"
  ],
  "namespaced_identifiers": false,
  "source": {
    "operation": "list",
    "properties": {
      "datatype": "{{@ datatype @}}"
    },
    "system": "{{@ system @}}",
    "type": "rest"
  },
  "transform": [
    {
      "rules": {
        "default": [
          [
            "copy",
            "*"
          ],
          [
            "merge",
            [
              "max",
              "_.",
              [
                "apply",
                "last-modified",
                "_S.changes"
              ]
            ]
          ],
          [
            "add",
            "_origin",
            [
              "first",
              [
                "hops",
                {
                  "datasets": [
                    "{{@ system @}}-projectcategory-share st"
                  ],
                  "return": "st.$origin",
                  "track-dependencies": false,
                  "where": [
                    [
                      "eq",
                      "_S.id",
                      "st.$generated_id"
                    ]
                  ]
                }
              ]
            ]
          ],
          [
            "if",
            [
              "is-not-null",
              "_T._origin"
            ],
            [
              "add",
              "$origin",
              "_T._origin"
            ]
          ]
        ],
        "last-modified": [
          [
            "filter",
            [
              "eq",
              "_S.changeType",
              "UPDATE"
            ]
          ],
          [
            "add",
            "$last-modified",
            [
              "datetime-parse",
              "%Y-%m-%dT%H:%M:%S%z",
              "_S.timestamp"
            ]
          ]
        ]
      },
      "type": "dtl"
    }
  ],
  "type": "pipe"
}