{
    "_id": "{{@ system @}}",
    "authentication": "basic",
    "operations": {
        "list-customer": {
            "method": "GET",
            "url": "customer"
        },
        "register-webhook": {
            "method": "POST",
            "payload": {
                "authHeaderName": "Authorization",
                "authHeaderValue": "Bearer $SECRET(service_jwt)"
            },
            "payload-type": "json",
            "url": "event/subscription"
        }
    },
    "password": "$SECRET(sessionToken)",
    "type": "system:rest",
    "url_pattern": "{{@ base_url @}}/v2/%s",
    "username": "0",
    "verify_ssl": true
}