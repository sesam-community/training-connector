{
  "_id": "xxxxxx-customer-share",
  "type": "pipe",
  "source": {
    "type": "dataset",
    "dataset": "xxxxxx-customer-transform"
  },
  "sink": {
    "deletion_tracking": false,
    "set_initial_offset": "onload"
  },
  "transform": [{
    "type": "template",
    "properties": {
      "client_side_patching": true,
      "operation_delete": "customer-delete",
      "operation_insert": "customer-insert",
      "operation_lookup": "customer-lookup",
      "operation_update": "customer-update",
      "primary_key": "id",
      "rest_system": "xxxxxx",
      "rewrite_rules_payload": {
        "rewrite_update_payload_after_patch": [
          ["add", "$payload",
            ["list", "_S._payload"]
          ]
        ]
      },
      "share_dataset": "xxxxxx-customer-share"
    },
    "template": "transform-share-rest"
  }],
  "batch_size": 1,
  "namespaced_identifiers": false
}
